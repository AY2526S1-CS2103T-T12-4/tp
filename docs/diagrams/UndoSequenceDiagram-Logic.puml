@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AbsolutSinemaParser" as AbsolutSinemaParser LOGIC_COLOR
participant "u:UndoCommand" as UndoCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box
[-> LogicManager : execute(undo)
activate LogicManager

LogicManager -> AbsolutSinemaParser : parseCommand(undo)
activate AbsolutSinemaParser

create UndoCommand
AbsolutSinemaParser -> UndoCommand
activate UndoCommand

UndoCommand --> AbsolutSinemaParser
deactivate UndoCommand

AbsolutSinemaParser --> LogicManager : u
deactivate AbsolutSinemaParser

LogicManager -> UndoCommand : execute()
activate UndoCommand

UndoCommand -> Model : undoAbsolutSinema()
activate Model

Model --> UndoCommand
deactivate Model

UndoCommand --> LogicManager : result
deactivate UndoCommand
UndoCommand -[hidden]-> LogicManager : result
destroy UndoCommand

[<--LogicManager
deactivate LogicManager
@enduml
